{% for event in events %}
{% if isValidActor(event.originatingActor) %}
  {% if event.type == "dialogue" or event.type == "dialogue_npc" or event.type == "dialogue_player" or event.type == "dialogue_player_stt" or event.type == "dialogue_player_text" or event.type == "dialogue_background" %}
    {% if event.originatingActor == npc.UUID %}
[ assistant ]
{{ event.data }}
[ end assistant ]
    {% else %}
[ user ]
{% if event.type == "dialogue_background" %}[Background] {% endif %}{{ decnpc(event.originatingActor).name }}{% if isValidActor(event.targetActor) %} (to {{ decnpc(event.targetActor).name }}){% endif %}: {{ event.data }}
[ end user ]
  {% endif %}
{% endif %}
{% endif %}
{% endfor %}
