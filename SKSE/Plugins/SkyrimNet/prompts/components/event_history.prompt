{% for event in events %}
{% if isValidActor(event.originatingActor) %}
    {% if event.type == "dialogue" or event.type == "dialogue_npc" or event.type == "dialogue_player" or event.type == "dialogue_player_stt" %}
        {% if event.originatingActor == npc.UUID %}
[ assistant ]
{{ event.data }}
[ end assistant ]
        {% else %}
[ user ]
{{ decnpc(event.originatingActor).name }}: {{ event.data }}
[ end user ]
        {% endif %}
    {% endif %}
{% endif %}
{% endfor %}
